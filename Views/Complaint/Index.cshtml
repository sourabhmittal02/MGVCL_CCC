      } else if ($('#BillingComplaint').prop('checked')) {
          Complainttype = 2;
          $('h5').text('Complaint Type => Billing Complaint');
      } else if ($('#MeterRelatedComplaint').prop('checked')) {
          Complainttype = 3;
          $('h5').text('Complaint Type => Meter Related Complaint');
      } else if ($('#PowerComplaint').prop('checked')) {
          Complainttype = 4;
          $('h5').text('Complaint Type => Power Complaint');
      } else if ($('#OtherComplaint').prop('checked')) {
          Complainttype = 5;
          $('h5').text('Complaint Type => Other Complaint');
      } else if ($('#SafetyComplaint').prop('checked')) {
          Complainttype = 6;
          $('h5').text('Complaint Type => Safety Complaint');
      } else if ($('#ServiceConn.Complaint').prop('checked')) {
          Complainttype = 7;
          $('h5').text('Complaint Type => Service Conn. Complaint');
      } else if ($('#TransformerRelated').prop('checked')) {
          Complainttype = 8;
          $('h5').text('Complaint Type => Transformer Related');
      } else if ($('#TheftComplaint').prop('checked')) {
          Complainttype = 9;
          $('h5').text('Complaint Type => Theft Complaint');
      } else if ($('#QualityofPowerComplaint').prop('checked')) {
          Complainttype = 10;
          $('h5').text('Complaint Type => Quality of Power Complaint');
      } else if ($('#OverHeadLinesComplaint').prop('checked')) {
          Complainttype = 11;
          $('h5').text('Complaint Type => Over Head Lines Complaint');
      } else if ($('#PaymentComplaint').prop('checked')) {
          Complainttype = 12;
          $('h5').text('Complaint Type => Payment Complaint');
      } else if ($('#SolarComplaint').prop('checked')) {
          Complainttype = 13;
          $('h5').text('Complaint Type => Solar Complaint');
      } else if ($('#RefundRelatedComplaint').prop('checked')) {
          Complainttype = 14;
          $('h5').text('Complaint Type => Refund Related Complaint');
      } else if ($('#SmartMeterRelatedcomplaints').prop('checked')) {
          Complainttype = 15;
          $('h5').text('Complaint Type => Smart Meter Related complaints');
      }
      $('#ComplaintTypeId').val(Complainttype);
      dataObject.COMPLAINT_NO = $('#txtComplaintno').val();
      dataObject.KNO = $('#txtsearchKno').val();

      dataObject.ACCOUNT_NO = $('#txtaccountNo').val();
      dataObject.MOBILE_NO = $('#txtmobileno').val();
      dataObject.OFFICE_ID = $('#ddlOfficecode').val();

      dataObject.COMPLAINT_SOURCE = $('#ddlSource').val();
      dataObject.COMPLAINT_status = $('#ddlCurrentStatus').val();
      dataObject.assigned_status = $('#ddlCurrentassigned').val();
      dataObject.fromdate = $('#fromDate').val();
      dataObject.todate = $('#toDate').val();
     /* dataObject.currentStatus = $('#COMPLAINT_status_ID').val();*/
      dataObject.Complaint_Type = Complainttype;
      var RoleId = '@Request.RequestContext.HttpContext.Session["Roll_ID"]';
      myTable = $('#datatables-buttons').dataTable({
          "ajax": {
              "url": "/Complaint/GetComplaintSearch/",
              "type": "POST",
              "datatype": "json",
              "data": function (d) {
                  d.COMPLAINT_NO = dataObject.COMPLAINT_NO;
                  d.KNO = dataObject.KNO;
                  d.ACCOUNT_NO = dataObject.ACCOUNT_NO;
                  d.MOBILE_NO = dataObject.MOBILE_NO;
                  d.OFFICE_ID = dataObject.OFFICE_ID;
                  d.COMPLAINT_SOURCE = dataObject.COMPLAINT_SOURCE;
                  d.COMPLAINT_status = dataObject.COMPLAINT_status;
                  d.assigned_status = "0";
                  d.fromdate = dataObject.fromdate;
                  d.todate = dataObject.todate;
                  d.currentStatus = dataObject.currentStatus;
                  d.Complaint_Type = dataObject.Complaint_Type;
              }
          },
          "destroy": true,
          "lengthMenu": [[10, 20, 50, 100, 150], [10, 20, 50, 100, 150]],
          "pageLength": 10,
          "processing": true,
          "serverSide": true,
          scrollY: true,
          scrollX: true,
          "searching": false,
          ordering: false,
          "language": {
              "processing": '<i class="fa fa-spinner fa-spin fa-4x fa-fw" style="color:#3B3B96;"></i><span class="sr-only">Loading...</span>'
              /*"processing": '<div class="spinner-border text-primary" role="status" ><span class="visually-hidden">Loading...</span></div>'*/
              /* "processing":  '<div class= "d-flex align-items-center" >  <strong>Loading...</strong>  <div class="spinner-border ms-auto" role="status" aria-hidden="true"></div></div>'*/
              /*"processing": "<div class='overlay custom-loader-background'><i class='fa fa-cog fa-spin custom-loader-color'></i></div>"  */
          },
          dom: 'Blfrtip',
          buttons: [
              'colvis'
          ],
          "columns": [
              {
                  'width': 120,
                  data: 'COMPLAINT_NO',
                  name: 'COMPLAINT_NO',
                  render: function (data, type, row, meta) {
                      return '<a class=" d-inline-block fw-normal w-100 h-100 pe-auto" href="#" onclick="complaintShow(' + row.COMPLAINT_NO + ')">' + row.COMPLAINT_NO + '</a>  <a class="fas fa-fw fa-pen" href="#" onclick=editComplaintShow(' + row.COMPLAINT_NO + ') />'
                  },
              },
              { 'data': 'KNO'},
              { 'data': 'NAME' },
              { 'data': 'COMPLAINT_DATE'  , 'width': 120 },
              { 'data': 'COMPLAINT_status' },
              { 'data': 'Current_status' },
              { 'data': 'DURATION' },
              { 'data': 'OFFICE_NAME' },
              { 'data': 'ADDRESS' },
              { 'data': 'MOBILE_NO' },
              { 'data': 'ALTERNATE_MOBILE_NO' },
              { 'data': 'COMPLAINT_TYPE' },
              { 'data': 'REMARK' },
              {
                  data: 'COMPLAINT_NO',
                  name: '',
                  render: function (data, type, row, meta) {
                      if (row.COMPLAINT_status != "Closed") {
                          return '<button href="#" type="submit" class="btn btn-primary btnShadow" onclick="Addremark(' + row.COMPLAINT_NO + ')">Remark</a>'
                      }
                      else { return '' }
                  },
              },
              {
                  data: 'COMPLAINT_NO',
                  name: '',
                  render: function (data, type, row, meta) {
                      if (row.COMPLAINT_status != "Closed" && (id == 2)) {
                          return '<button href="#" type="submit" class="btn btn-success btnShadow" onclick="ChangeComplaint(' + row.COMPLAINT_NO + ')">Complaint</a>'
                      } else { return '' }
                  },
              },
              {
                  data: 'COMPLAINT_NO',
                  name: '',
                  render: function (data, type, row, meta) {
                      if (row.COMPLAINT_status != "Closed" && (id == 2 || id == 3)) {
                          return '<button href="#" type="submit" class="btn btn-warning btnShadow" onclick="Assign(' + row.COMPLAINT_NO + ')">Assign</a>'
                      } else { return '' }
                  },
              },
              {
                  data: 'COMPLAINT_NO',
                  name: '',
                  render: function (data, type, row, meta) {
                      if ((row.COMPLAINT_status != "Closed" && (id == 2)) || (row.COMPLAINT_status != "Closed" && (row.COMPLAINT_TYPE == "Billing Complaint" || row.COMPLAINT_TYPE == "Payment Complaint" || row.COMPLAINT_TYPE == "Solar Complaint" || row.COMPLAINT_TYPE == "Refund Related Complaint") && (id == 5)))  {
                          return '<button href="#" type="submit" class="btn btn-danger btnShadow" onclick="Close(' + row.COMPLAINT_NO + ')">Close</a>'
                      } else { return '' }
                  },
              },
              {
                  'width': 180,
                  data: 'COMPLAINT_NO',
                  name: '',
                  render: function (data, type, row, meta) {
                      if (row.COMPLAINT_TYPE == "New Connection" && (id == 2 || id == 1)) {
                          return '<button href="#" type="submit" class="btn btn-info btnShadow"  onclick="NewConnection(' + row.COMPLAINT_NO + ')">New Connection</a>'
                      } else { return '' }
                       /*return '<button href="#" type="submit" class="btn btn-danger btnShadow"  onclick="NewConnection(' + row.COMPLAINT_NO + ')">New Connection</a>'*/
                  },

              },
              { 'data': 'SOURCE_NAME' },
              { 'data': 'ASSIGNED_TO' },
              { 'data': 'CLOSED_BY' },
          ],
          "rowCallback": function (row, data, dataIndex) {
              if (data['COMPLAINT_status'] == "Pending") {
                  $(row).find('td:eq(4)').addClass('table-warning');
              } else if (data['COMPLAINT_status'] == "Closed") {
                  $(row).find('td:eq(4)').addClass('table-success');
              }

          }

      });

  }


</script> 

