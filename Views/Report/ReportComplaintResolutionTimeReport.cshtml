                            new SelectListItem{ Text="2025", Value = "2025" },
                         }, new { @class = "form-control select2", id = "ddlBillyear" })
                    </div>
                    
                    <div class="col-12 col-lg-2">
                        <br />
                        <input type="submit" value="Search" class="btn btn-dark btn-block" id="btnSearch" onclick="searchreq()" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
