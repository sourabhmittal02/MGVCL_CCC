        dataObject.BILL_MONTH = $('#ddlBillMonth').val();
        dataObject.BILL_YEAR = $('#ddlBillyear').val();
        $.ajax({
            type: "GET",
            url: '/Report/ReportComplaintResolutionHoursWiseSearch',
            data: dataObject,
            dataType: "json",
            async: "true",
            success: function (data) {
                var datatablesButtons = $('#datatables-buttons').DataTable(
                    {
